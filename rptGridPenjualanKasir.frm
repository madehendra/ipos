Attribute VB_Name = "rptGridPenjualanKasir"
Attribute VB_GlobalNameSpace = False
Attribute VB_Creatable = False
Attribute VB_PredeclaredId = True
Attribute VB_Exposed = False
Option Explicit
Dim dbData As New ADODB.Recordset
Dim objData As New CodeSuiteLibrary.Data
Dim nLebarCol1 As Double
Dim nLebarCol2 As Double
Dim nLebarCol3 As Double
Dim nLebarCol4 As Double
Dim nLebarCol5 As Double
Dim nLebarCol6 As Double
Dim nLebarCol7 As Double
Dim nTotalLebar As Double
Dim vaArray As New XArrayDB

Private Sub Check1_KeyDown(KeyCode As Integer, Shift As Integer)
  If KeyCode = vbKeyReturn Then
    keybd_event VK_TAB, 0, 0, 0
  End If
End Sub

Private Sub cKasir_ButtonClick()
  Set dbData = objData.Browse(GetDSN, "username", "username,fullname", , , , , "username")
  If Not dbData.EOF Then
    cKasir.Text = cKasir.Browse(dbData)
  End If
End Sub


Private Sub cmdKeluar_Click()
  Unload Me
End Sub

Private Sub cmdPreview_Click()
Dim n As Integer
Dim cSQL As String
Dim nTotal As Double

  nTotal = 0
  cSQL = ""
  If Check1.Value = 1 Then
    cSQL = "AND username = '" & cKasir.Text & "'"
  End If
  vaArray.ReDim 0, -1, 0, 6
  Set dbData = objData.Browse(GetDSN, "totkasir", "tgl,username,nomorkasir,subtotal,discount,total", "tgl", sisGTEqual, Format(dTgl(0).Value, "yyyy-MM-dd"), " AND tgl <= '" & Format(dTgl(1).Value, "yyyy-MM-dd") & "'" & cSQL, "tgl,username,nomorkasir")
  If Not dbData.EOF Then
    Do While Not dbData.EOF
      vaArray.InsertRows vaArray.UpperBound(1) + 1
      n = vaArray.UpperBound(1)
      vaArray(n, 0) = n + 1
      vaArray(n, 1) = Format(GetNull(dbData!tgl), "dd-MM-yyyy")
      vaArray(n, 2) = GetNull(dbData!UserName)
      vaArray(n, 3) = GetNull(dbData!nomorkasir)
      vaArray(n, 4) = GetNull(dbData!Subtotal)
      vaArray(n, 5) = GetNull(dbData!Discount)
      vaArray(n, 6) = GetNull(dbData!Total)
      nTotal = nTotal + vaArray(n, 6)
      dbData.MoveNext
    Loop
  End If
  DataGrid1.Columns(6).FooterText = Format(nTotal, "###,###,###,##0.00")
  Set DataGrid1.Array = vaArray
  DataGrid1.ReBind
  DataGrid1.Refresh
End Sub

Private Sub Form_Activate()
  cmdPreview_Click
End Sub

Private Sub Form_Load()
Dim n As Single

  TabIndex dTgl(0), n
  TabIndex dTgl(1), n
  TabIndex Check1, n
  TabIndex cKasir, n
  TabIndex cmdPreview, n
  TabIndex cmdKeluar, n
  
  CenterForm Me
  SetIcon Me.hWnd
  Me.Refresh
  DataGrid1.Refresh
  nLebarCol1 = DataGrid1.Columns(0).Width
  nLebarCol2 = DataGrid1.Columns(1).Width
  nLebarCol3 = DataGrid1.Columns(2).Width
  nLebarCol4 = DataGrid1.Columns(3).Width
  nLebarCol5 = DataGrid1.Columns(4).Width
  nLebarCol6 = DataGrid1.Columns(5).Width
  nLebarCol7 = DataGrid1.Columns(6).Width
  nTotalLebar = nLebarCol1 + nLebarCol2 + nLebarCol3 + nLebarCol4 + nLebarCol5 + nLebarCol6 + nLebarCol7
End Sub

Private Sub InitValue()
  vaArray.ReDim 0, -1, 0, 6
  Set DataGrid1.Array = vaArray
  DataGrid1.Refresh
  DataGrid1.ReBind
  DataGrid1.Columns(6).FooterText = 0
End Sub

Private Sub Form_Resize()
  If Me.WindowState = 2 Then
    Me.Refresh
    DataGrid1.Columns(0).Width = nLebarCol1 / nTotalLebar * DataGrid1.Width
    DataGrid1.Columns(1).Width = nLebarCol2 / nTotalLebar * DataGrid1.Width
    DataGrid1.Columns(2).Width = nLebarCol3 / nTotalLebar * DataGrid1.Width
    DataGrid1.Columns(3).Width = nLebarCol4 / nTotalLebar * DataGrid1.Width
    DataGrid1.Columns(4).Width = nLebarCol5 / nTotalLebar * DataGrid1.Width
    DataGrid1.Columns(5).Width = nLebarCol6 / nTotalLebar * DataGrid1.Width
    DataGrid1.Columns(6).Width = nLebarCol7 / nTotalLebar * DataGrid1.Width
    DataGrid1.Refresh
  Else
    Me.Refresh
    DataGrid1.Columns(0).Width = nLebarCol1
    DataGrid1.Columns(1).Width = nLebarCol2
    DataGrid1.Columns(2).Width = nLebarCol3
    DataGrid1.Columns(3).Width = nLebarCol4
    DataGrid1.Columns(4).Width = nLebarCol5
    DataGrid1.Columns(5).Width = nLebarCol6
    DataGrid1.Columns(6).Width = nLebarCol7
    DataGrid1.Refresh
  End If
End Sub
